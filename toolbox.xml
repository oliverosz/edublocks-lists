<category name="Lists" colour="#38bdf8" css-icon="customIcon fas fa-list">
    <label text="Lists"></label>

    <block type="list_create_empty"></block>

    <block type="list_item_assign">
        <value name="idx">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
        <value name="val">
            <shadow type="inline_text">
                <field name="value">0</field>
            </shadow>
        </value>
    </block>

    <block type="matrix_item_assign">
        <value name="idx1">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
        <value name="idx2">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
        <value name="val">
            <shadow type="inline_text">
                <field name="value">0</field>
            </shadow>
        </value>
    </block>

    <block type="matrix_item_access">
        <value name="idx1">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
        <value name="idx2">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
    </block>

    <block type="procedure_call">
        <value name="function_name">
            <shadow type="inline_text">
                <field name="value">function_name</field>
            </shadow>
        </value>
        <value name="parameters">
            <shadow type="parameters">
                <mutation color="#9aa429" items="1" limit="100" inputType="text" />
                <value name="input_1">
                    <shadow type="inline_text">
                        <field name="value">params</field>
                    </shadow>
                </value>
            </shadow>
        </value>
    </block>

    <block type="procedure_call_no_args">
        <value name="function_name">
            <shadow type="inline_text">
                <field name="value">function_name</field>
            </shadow>
        </value>
    </block>

    <block type="function_call_no_args">
        <value name="function_name">
            <shadow type="inline_text">
                <field name="value">function_name</field>
            </shadow>
        </value>
    </block>
</category>
