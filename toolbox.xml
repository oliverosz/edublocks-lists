<category name="Extras" colour="#e83c40" css-icon="customIcon fas fa-asterisk">
    <label text="Extras"></label>

    <block type="list_create_empty"></block>

    <block type="list_item_assign">
        <value name="idx">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
        <value name="val">
            <shadow type="inline_text">
                <field name="value">0</field>
            </shadow>
        </value>
    </block>

    <block type="matrix_item_assign">
        <value name="idx1">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
        <value name="idx2">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
        <value name="val">
            <shadow type="inline_text">
                <field name="value">0</field>
            </shadow>
        </value>
    </block>

    <block type="matrix_item_access">
        <value name="idx1">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
        <value name="idx2">
            <shadow type="inline_number">
                <field name="value">0</field>
            </shadow>
        </value>
    </block>

    <block type="in_operator">
        <value name="val">
            <shadow type="inline_text">
                <field name="value">0</field>
            </shadow>
        </value>
        <value name="iterable"></value>
    </block>

    <block type="function_call">
        <value name="function_name">
            <shadow type="inline_text">
                <field name="value">function_name</field>
            </shadow>
        </value>
        <value name="parameters">
            <shadow type="parameters">
                <mutation color="#9aa429" items="1" limit="100" inputType="text" />
                <value name="input_1">
                    <shadow type="inline_text">
                        <field name="value"></field>
                    </shadow>
                </value>
            </shadow>
        </value>
    </block>

    <button text="Hide unused categories" callbackKey="hideCategories"></button>
</category>
